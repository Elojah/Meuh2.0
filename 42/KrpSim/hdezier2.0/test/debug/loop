# process   name:(need1:qty1;need2:qty2;[...]):(result1:qty1;result2:qty2;[...]):delay
#
make_sec:(clock:1):(clock:1;second:1):1
make_minute:(second:2):(minute:1):6
make_hour:(minute:2):(hour:1):36
#make_day:(hour:2):(day:1):86
#make_year:(day:2):(year:1):365
